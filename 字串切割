#include<iostream>
#include<string>
#include <sstream>
  using namespace std;
  int main(){
      string str;
      string fuck ="1,5,6,7,8,9";
      istringstream  shit(fuck);
      getline(shit, str, ',');
      cout<<"The string we have gotten is :"<<str<<endl;
      getline(shit, str, ',');
      cout<<"The string we have gotten is :"<<str<<endl;
      return 0;
	       }
